{
  "root": true,
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
  ],
  "env": {
    "es6": true,
    "browser": true,
  },
  "globals": {
    "module": true,
    "__dirname": true,
    "require": true,
    "process": true,
  },
  "rules": {
    "no-var": 0,
    "no-unused-vars": 0,
    "no-underscore-dangle": 0,
    "no-inner-declarations": 0,
    "comma-dangle": 0,
    "prefer-arrow-callback": 0,
    "no-console": 0,
    "no-continue": 0,
    "object-shorthand": 0,
    "no-param-reassign": 0,
    "vars-on-top": 0,
    "func-names": 0,
    "consistent-return": 0,
    "global-require": 0,
  }
}
