{
  "name": "kiku",
  "version": "1.3.0",
  "author": "hiro <hiro@0218.jp>",
  "homepage": "https://b.0218.jp/",
  "repository": {
    "type": "git",
    "url": "git://github.com/hiro0218/kiku-theme.git"
  },
  "bugs": {
    "url": "https://github.com/hiro0218/kiku-theme/issues"
  },
  "license": "MIT",
  "keyword": [
    "wordpress",
    "theme"
  ],
  "scripts": {
    "build": "webpack --mode development --progress --config src/build/webpack.config.js",
    "build:production": "webpack --mode production --progress -p --config src/build/webpack.config.js",
    "build:profile": "webpack --progress --profile --json --config src/build/webpack.config.js",
    "start": "webpack --hide-modules --watch --config src/build/webpack.config.js",
    "clean": "rimraf dist",
    "lint:js": "eslint --ext .js,.vue src/scripts/* src/components/*",
    "lint:sass": "sass-lint -c .sass-lint.yml -q -v",
    "format:js": "eslint --fix --ext .js,.vue src/scripts/* src/components/*",
    "format:php": "vendor/bin/phpcbf",
    "php7cc": "vendor/bin/php7cc lib/",
    "precommit": "lint-staged",
    "test": "yarn run lint -s"
  },
  "lint-staged": {
    "**/*.{js,vue}": [
      "eslint --fix",
      "eslint",
      "git add"
    ]
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.2",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
